<!DOCTYPE html>
<html>
  <head>

    <% if current_user %>
    <%= current_user.email %>
    <%= button_to("Logout", session_url, method: :delete) %>
    <% end %>

    <title>MusicApp</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <%= yield %>
  </body>
</html>
