<h1>Cats upon cats!</h1>

<ul>
  <% @cats.each do |cat| %>
    <li><%= link_to cat.name, cat_url(cat) %></li>
  <% end %>
</ul>

<% if current_user %>
  <%= link_to "Create cat", new_cat_url %>
<% else %>
  <%= link_to "Sign in", new_sessions_url %>
  <%= link_to "Sign up", new_user_url %>
<% end %>
